<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lawrenceqiu.yilaword.app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".vocablist.MainActivity"
            android:label="@string/app_name"
                android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".registerlogin.RegisterLoginActivity" />
        <activity android:name=".scrambledword.ScrambledWordActivity" />
        <activity android:name=".findword.FindWordActivity" android:screenOrientation="portrait"/>
        <activity android:name=".WebDictionaryActivity"/>
    </application>

</manifest>
